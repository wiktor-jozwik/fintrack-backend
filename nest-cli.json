{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "compilerOptions": {
    "plugins": [
      "@nestjs/swagger"
    ],
    "assets": [
      "**/*.hbs"
    ],
    "watchAssets": true,
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "money-tracker-api-nestjs": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "currency-rates-importer": {
      "type": "application",
      "root": "apps/currency-rates-importer",
      "entryFile": "main",
      "sourceRoot": "apps/currency-rates-importer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/currency-rates-importer/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "operations-import-handler": {
      "type": "application",
      "root": "apps/operations-import-handler",
      "entryFile": "main",
      "sourceRoot": "apps/operations-import-handler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/operations-import-handler/tsconfig.app.json"
      }
    }
  }
}